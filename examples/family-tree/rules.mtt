@prefix schema: <https://schema.org/> .

($child1 schema:sibling $child2)
    → ($child2 schema:sibling $child1) .

($person1 schema:spouse $person2)
    → ($person2 schema:spouse $person1) .

($child schema:parent $parent)
    → ($parent schema:children $child) .

($parent schema:children $child)
    → ($child schema:parent $parent) .

($child1 schema:parent $parent), ($child2 schema:parent $parent)
    → ($child1 schema:sibling $child2) .